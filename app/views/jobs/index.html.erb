<h1>Job listings!</h1>

<% @jobs.each do |job| %>
  <div>
    <h3><%= job.title %></h3>
    <h4><%= job.location %></h4>
    <h5><%= job.business_name %></h5> 
    <p><%= job.description %></p>
  	<a href="/jobs/<%= job.id %>">View Job</a>

    <!-- How do I get the post date in here? -->
  </div>
  <br>
<% end %>